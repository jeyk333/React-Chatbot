    "actions-on-google": "^2.12.0",
    "body-parser": "^1.20.1",
    "concurrently": "^4.1.2",
    "dialogflow": "^0.10.3",
    "dialogflow-fulfillment": "^0.6.1",
    "express": "^4.18.2",
    "mongoose": "^5.6.11",
    "nodemon": "^1.19.1",
    "structjson": "^1.0.1"
  }
}
